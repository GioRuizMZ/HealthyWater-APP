<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/txt_red"
        android:layout_width="237dp"
        android:layout_height="42dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="9dp"
        android:layout_marginBottom="9dp"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="false"
        android:focusable="false"
        android:fontFamily="@font/poppins_medium"
        android:padding="8dp"
        android:text="Nombre de tu red"
        android:textColor="@android:color/black"
        android:textSize="16sp" />
    <ImageView
        android:id="@+id/wifi_seguridad"
        android:layout_marginTop="15dp"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/wifi_sin_contra"
        />
    <ImageView
        android:id="@+id/wifi_intensidad"
        android:layout_marginTop="13dp"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:src="@drawable/wifi_alto"
        />
    <TextView
        android:id="@+id/textview_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text=">"
        android:textColor="@color/boton"
        android:textSize="22sp" />
</LinearLayout>